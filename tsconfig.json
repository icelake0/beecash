{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@/app/*": ["./app/*"],
      "@/components/*": ["components/*"],
      "@/components": ["components"],
      "@/components/icons": ["components/icons"],
      "@/hooks/*": ["hooks/*", ""],
      "@/stores/*": ["stores/*"],
      "@/stores": ["stores"],
      "@/services/*": ["services/*"],
      "@/services": ["services"],
      "@/endpoints/*": ["endpoints/*"]
    },
    "types": ["./utils/theme/styled.d.ts"]
  },
  "include": [
    "app/**/*.d.ts",
    "app/**/*.ts",
    "app/**/*.tsx",
    "tests/**/*.ts",
    "tests/**/*.tsx",
    "components/**/*.d.ts",
    "components/**/*.ts",
    "components/**/*.tsx",
    "components/**/*.ts",
    "components/*.ts",
    "components/*.tsx",
    "components/**/*",
    "hooks/**/*.tsx",
    "hooks/**/*.ts",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "utils/theme/styled.d.ts",
    "*.d.ts",
    "types/**/*.ts"
  ]
}
